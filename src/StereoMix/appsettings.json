{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting": "Information",
      "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Information",
      "Grpc": "Information",
      "StereoMix": "Debug"
    }
  },
  "AllowedHosts": "*",
  "StereoMix": {
    "JWT": {
      "Issuer": "StereoMixLiveGrpcServer",
      "Audience": {
        "GameServer": "StereoMixLiveGameServer",
        "Client": "StereoMixLiveClient"
      }
    }
  },
  "Edgegap": {
    "DeploymentOption": {
      "Filter": {
        "Field": "Country",
        "FilterType": "Any",
        "Values": [
          "Sound Korea"
        ]
      }
    }
  }
}
